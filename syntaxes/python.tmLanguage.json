{
  "scopeName": "source.python",
  "name": "Python (MicroPython)",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#builtins"
    },
    {
      "include": "#comments"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.python",
          "match": "\\b(?:and|as|assert|async|await|break|class|continue|def|del|elif|else|except|False|finally|for|from|global|if|import|in|is|lambda|None|nonlocal|not|or|pass|raise|return|True|try|while|with|yield|Error|MemoryError|BaseException|bb_port|ti_rover|ti_hub|escape|Exception|mem32|mem16|mem8)\\b"
        }
      ]
    },
    "builtins": {
      "patterns": [
        {
          "name": "support.function.builtin.python",
          "match": "\\b(?:print|len|range|input|type|dir|id|help|ti_system|ti_hub|isti|version|begin|start|about|what|who|last_error|bb_port|ti|ti_rover|min|max)\\b"
        }
      ]
    },
    "comments": {
      "patterns": [
        {
          "name": "comment.line.number-sign.python",
          "match": "#.*$"
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.single.python",
          "begin": "'",
          "end": "'"
        },
        {
          "name": "string.quoted.double.python",
          "begin": "\"",
          "end": "\""
        }
      ]
    }
  },
  "fileTypes": ["py", "mpy"],
  "uuid": "micro-python-grammar"
}